sole.log(error));\n  };\n\n  useEffect(() => {\n    fetchLatestMovies();\n    fetchMainImages();\n  }, []);\n\n  return (\n    <div className=\"container-fluid d-flex justify-content-center flex-column \">\n      <div className=\"container\">{/* <ImageSlider /> */}</div>\n\n      <div className=\"row\">\n        <div className=\"col-12\">\n          {latestMovies[0] && (\n            <MainImage\n              image={`${IMAGE_URL}w1280${latestMovies[0].poster_path}`}\n              title={latestMovies[0].title}\n              overView={latestMovies[0].overview}\n            />\n          )}\n        </div>\n      </div>\n\n      <div>\n        <h2>The Latest Movies</h2>\n      </div>\n\n      <Scroll>\n        <div className=\"container-fluid d-flex justify-content-center flex-wrap\">\n          {latestMovies.map((movie) => (\n            <Movie\n              image={\n                movie.poster_path && `${IMAGE_URL}w200${movie.poster_path}`\n              }\n              key={movie.id}\n              id={movie.id}\n              title={movie.title}\n              poster_path={movie.poster_path}\n              overview={movie.overview}\n              vote_average={movie.vote_average}\n            />\n          ))}\n        </div>\n      </Scroll>\n    </div>\n  );\n};\n\nexport default LandingPage;\n","/Users/shane_qi/Development/movie-web-project/src/pages/PopularMovies.js",[],"/Users/shane_qi/Development/movie-web-project/src/pages/TopRatedMovies.js",[],"/Users/shane_qi/Development/movie-web-project/src/Config.js",[],"/Users/shane_qi/Development/movie-web-project/src/components/Movie.js",[],"/Users/shane_qi/Development/movie-web-project/src/components/Pagination.js",["76","77","78","79","80"],"/Users/shane_qi/Development/movie-web-project/src/components/MainImage.js",[],"/Users/shane_qi/Development/movie-web-project/src/components/Scroll.js",[],"/Users/shane_qi/Development/movie-web-project/src/hooks/hooks.js",[],{"ruleId":"81","severity":1,"message":"82","line"